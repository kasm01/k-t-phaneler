  --image=$IMAGE \
  --region=europe-west2 \
  --platform=managed \
  --set-secrets=BINANCE_API_KEY=BINANCE_API_KEY:latest \
  --set-secrets=BINANCE_API_SECRET=BINANCE_API_SECRET:latest \
